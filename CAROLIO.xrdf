        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/CAROLIO_0000411"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/CAROLIO_0000412"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/CAROLIO_0000413"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/CAROLIO_0000414"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CAROLIO_0000400"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/CAROLIO_0004111"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>is a level of pain that is further separated into &apos;mild pain&apos;, &apos;moderate pain&apos;, &apos;no pain&apos;, or &apos;severe pain&apos; based on the realted Data Property:has recurrent value.</obo:IAO_0000115>
        <obo:IAO_0000119>Pain scale:https://www.healthline.com/health/pain-scale#types</obo:IAO_0000119>
        <oboInOwl:hasOBONamespace>carolio</oboInOwl:hasOBONamespace>
        <oboInOwl:id>CAROLIO:0000410</oboInOwl:id>
        <rdfs:label>pain scale</rdfs:label>
        <skos:notation>CAROLIO:0000410</skos:notation>
    </owl:Class>
    


